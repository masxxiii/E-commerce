<%- include("headerHome") -%>

        <div class="container">
            <div class="row">

                <%  foundProducts.forEach(function(item){ %>

                <div class="col-lg-3 col-md-4 col-sm-6">
                    <div class="product-grid2">
                        <div class="product-image2"> <a href="#"> <img class="pic-1" src="<%=  item.link1  %>"> <img class="pic-2" src="<%=  item.link2  %>"> </a>
                            <ul class="social">
                                <li><a href="/login" data-tip="Wishlist"><i class="fa fa-heart"></i></a></li>
                                <li><a href="/login" data-tip="Add to Cart"><i class="fa fa-shopping-cart"></i></a></li>
                            </ul>
                        </div>
                        <div class="product-content">
                            <h3 class="title"><a href="/<%= item._id %>"> <%= item.name %> </a></h3> <span class="price"> <%= item.price %> </span>
                        </div>
                    </div>
                </div>

                <% }) %>

            </div>
        </div>

</body>
</html>